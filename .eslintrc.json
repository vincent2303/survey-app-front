{
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    },
    "rules": {
      "valid-jsdoc": 2,
      "react/jsx-uses-react": 1,
      "react/jsx-no-undef": 2,
      "react/jsx-wrap-multilines": 2,
      "react/no-string-refs": 0,
      "react/prop-types": 0,
      "quotes": 0,
      "eol-last": 0,
      "func-names": 0,
      "camelcase": 0,
      "no-plusplus": 0,
      "no-console": 0,
      "consistent-return": 0,
      "prefer-destructuring": 0,
      "object-shorthand": 0,
      "no-param-reassign": 0,
      "no-trailing-spaces": 0,
      "import/no-extraneous-dependencies": 0,
      "prefer-arrow-callback": 0,
      "no-case-declarations": 0
    },
    "plugins": [
      "import",
      "react"
    ]
  }